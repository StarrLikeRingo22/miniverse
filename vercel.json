{
    "rewrites": [
      { "source": "/Three", "destination": "/Three.html" },
      { "source": "/Classic", "destination": "/Classic.html" },
      { "source": "/Classic/Resume", "destination": "/Classic/resume.html" },
      { "source": "/Attributions", "destination": "/attributions.html" }
    ],
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.jsx",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }